{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "omnifocus",
  "title": "OmniFocus",
  "description": "",
  "icon": "command-icon.png",
  "author": "potsbo",
  "license": "MIT",
  "commands": [
    {
      "name": "perspectives",
      "title": "Show Perspectives",
      "description": "Show OmniFocus Perspectives",
      "mode": "view"
    },
    {
      "name": "create",
      "title": "Create Task",
      "description": "Create a New Task",
      "mode": "view"
    },
    {
      "name": "inbox",
      "title": "Show Inbox",
      "description": "Show OmniFocus Inbox",
      "mode": "view"
    },
    {
      "name": "projects",
      "title": "Show Projects",
      "description": "Show OmniFocus Projects",
      "mode": "view"
    },
    {
      "name": "tags",
      "title": "Tags",
      "description": "Show OmniFocus Tags",
      "mode": "view"
    },
    {
      "name": "forecast",
      "title": "Forecast",
      "description": "Show OmniFocus Forecast",
      "mode": "view"
    },
    {
      "name": "flagged",
      "title": "Flagged",
      "description": "Show OmniFocus Flagged",
      "mode": "view"
    },
    {
      "name": "review",
      "title": "Review",
      "description": "Show OmniFocus Review",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.3",
    "@jacobx1/of-sdk": "^0.2.7",
    "@jxa/run": "^1.3.4",
    "@raycast/api": "^1.27.1",
    "graphql": "^16.3.0",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.14"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.5.0",
    "@graphql-codegen/typescript": "2.4.3",
    "@graphql-codegen/typescript-graphql-request": "^4.3.4",
    "@graphql-codegen/typescript-operations": "2.2.4",
    "@graphql-codegen/typescript-react-query": "^3.5.1",
    "@graphql-codegen/typescript-resolvers": "2.5.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "express": "^4.17.2",
    "express-graphql": "^0.12.0",
    "jest": "^27.5.0",
    "npm-watch": "^0.11.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "watch": {
    "playground": {
      "patterns": [
        "src"
      ],
      "extensions": "ts"
    }
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "concurrently \"ray develop\" \"yarn generate --watch\" \"npm-watch playground\"",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "generate": "graphql-codegen --config codegen.yml",
    "playground": "ts-node ./src/playground/index.ts"
  }
}
